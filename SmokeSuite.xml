<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke Suite">
    <test name="Smoke Test" parallel="methods" thread-count="2">
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>
        <classes>
            <class name="net.phpTravels.tests.CommonFeatureTests"/>
            <class name="net.phpTravels.tests.HomePageTests"/>
            <class name="net.phpTravels.tests.LoginPageTest"/>
            <class name="net.phpTravels.tests.SignUpPageTest"/>
        </classes>
    </test>
</suite>